{
                id: "ideas-R-language",
                extension: "r",
                name: "R Language",
                formats: [
                     {
                         format: "R_format",
                         editorModeId: "ace/mode/R",
                         _editorModeURI: "mode-R.js",
                         editorThemeId: "ace/theme/R",
			 _editorThemeURI: "theme-R.js",                         
                         checkLanguage: "true" 
                     }
                ],

                operations: [

                             {
                                 id: "executeScript",
                                 name: "Execute R script",
                                 data: {},
                                 _remoteExecution: "false",
                                 action: "function(operationStructure) { 
                                 var res;
                                 var fileUri= EditorManager.currentUri;
                                  var temp;
                                  CommandApi.doDocumentOperation(operationStructure.id,operationStructure.data,fileUri, function(result){
                                  	  temp=result.context;
                                  	 $.ajax('file/copyProjectToTemp?temDirectory='+temp+'&fileUri='+fileUri, {
                                  	 'type' : 'get', 
                                  	 'data' : {},
                                   	'success' : function(result) {
                                   		CommandApi.doDocumentOperation('executeScript2',operationStructure.data,fileUri, function(result){
                                   		
                                   		$.ajax('file/copyTemptoProject?temDirectory='+temp+'&fileUri='+fileUri, {
											                    'type' : 'get', 
                    											'data' : operationStructure.id,
                    											'success' : function(result) {
                                              CommandApi.doDocumentOperation('deleteTemp',operationStructure.data,fileUri,function(result){},true);

                                          },
                    				'onError' : function(result) {console.log('[ERROR] '+result);},
                                    'async' : true,});
                                    
                                    $.ajax('/ideas-R-language/inspector/environment',{
																					  'type':'post',
																					  'data':{},
																					  'success':function(result){$('#editorInspectorLoader').load(result.context);},
																					  'onError':function(result){console.log([ERROR]+result.context);},
																					  'async':true,
																					  });
                                    
                                   	},true);},
                                   	'onError' : function(result) {console.log('[ERROR] '+result);},
                                   	'async' : true,});
                                   	},true);
                                   		
                                   	}" 
                                   	
                             },
                             {
                                 id: "lint",
                                 name: "Lint",
                                 data: {},
                                 _remoteExecution: "true",
                                 action: "function(result) {}" 
                             },
                             {
                                 id: "endsession",
                                 name: "End session",
                                 data: {},
                                 _remoteExecution: "true",
                                 action: "function(result) {}" 
                             }

                             ],

                inspectorLoader: "function(loader, format) {}" 

            }